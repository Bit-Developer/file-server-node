  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/src/server.js",
    "start-config": "ts-node start-config",
    "local": "NODE_ENV=local nodemon ./src/server.ts --ignore ./uploads",
    "dev": "NODE_ENV=dev npm run start-config && nodemon ./src/server.ts --ignore ./uploads",
    "stage": "NODE_ENV=stage npm run start-config && nodemon ./src/server.ts --ignore ./uploads",
    "prod": "NODE_ENV=prod npm run start-config && npm run build && npm run start",
    "private": "NODE_ENV=private npm run start-config && npm run build && npm run start",
    "dev2": "ts-node ./src/server.ts",
    "build": "rimraf ./dist && tsc && cp -R -v src/public dist/src",
    "build-prod": "ts-node start-config-prod && rimraf ./dist && tsc && cp -R -v src/public dist/src",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest"
  },
  "private": true,
  "keywords": [],
  "dependencies": {
    "@types/app-root-path": "^1.2.4",

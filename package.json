    "@types/jest": "^26.0.15",
    "@types/mkdirp": "^1.0.1",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.14.2",
    "@types/serve-favicon": "^2.5.1",
    "app-root-path": "^3.0.0",
    "body-parser": "^1.19.0",

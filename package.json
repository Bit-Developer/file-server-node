    "cookie-parser": "^1.4.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "install": "^0.13.0",
    "jest": "^26.6.1",
    "jszip": "^3.5.0",
    "lint-staged": "^10.5.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
